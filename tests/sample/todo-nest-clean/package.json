{
  "name": "todo-nest-clean",
  "version": "0.1.0",
  "private": true,
  "description": "Clean Architecture Todo app using NestJS",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "swc src -d dist -s",
    "start:dev": "ts-node-dev --respawn --swc src/main.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@nestjs/common": "10.4.7",
    "@nestjs/core": "10.4.7",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1"
  },
  "devDependencies": {
    "@types/node": "22.7.4",
    "ts-node-dev": "2.0.0",
    "typescript": "5.6.3",
    "@swc/core": "1.7.39",
    "@swc/cli": "0.4.0",
    "@swc/register": "0.1.10"
  }
}
