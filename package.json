{
  "name": "arch-unit-js",
  "version": "1.0.0-beta.46",
  "description": "arch-unit clone for node.js projects",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build:tsc": "tsc",
    "build:types": "npx tsc --emitDeclarationOnly",
    "build": "node esbuild.config.js && npm run build:types",
    "test": "jest --detectOpenHandles --coverage",
    "test:watch": "jest --watch",
    "publish:beta": "npm publish --tag beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gftf2011/arch-unit-js.git"
  },
  "author": "Gabriel Ferrari Tarallo Ferraz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gftf2011/arch-unit-js/issues"
  },
  "homepage": "https://github.com/gftf2011/arch-unit-js#readme",
  "devDependencies": {
    "@types/babel__traverse": "7.20.7",
    "@types/jest": "^30.0.0",
    "@types/micromatch": "^4.0.9",
    "@types/node": "24.0.14",
    "esbuild": "^0.25.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@babel/parser": "7.28.0",
    "@babel/traverse": "7.28.0",
    "micromatch": "4.0.8",
    "upath": "2.0.1"
  }
}
